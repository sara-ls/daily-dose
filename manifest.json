{
  "manifest_version": 2,
  "name": "New Tab GIF Collection",
  "version": "1.2.0",
  "description": "Chrome Extension - gif Collection",
  "web_accessible_resources": ["images/*", "sounds/*"],
  "chrome_url_overrides": {
    "newtab": "manager.html"
  },
  "permissions": [
    "cookies",
    "tabs",
    "http://*/*",
    "https://*/*",
    "activeTab",
    "storage"
  ],
  "short_name": "Collect",
  "icons": {
    "192": "icon/android-chrome-192x192.png",
    "512": "icon/android-chrome-512x512.png"
  },
  "browser_action": {
    "default_icon": "icon/favicon-32x32.png",
    "default_popup": "popup.htm"
  },
  "background": {
    "scripts": ["background.js", "manager.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "exclude_globs": ["*/manager.html"],
      "css": ["fix.css"],
      "js": ["script.js", "jquery-3.4.1.min.js"]
    }
  ]
}
